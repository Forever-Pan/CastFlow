    0.0625
  ],
  ""flatline_ratio"": [
    0.9479166666666666
  ],
  ""flatline_hits"": [
    0.10416666666666667
  ],
  ""peak_count"": [
    3.0
  ],
  ""peak_max_width"": [
    6.317591103965697
  ],
  ""peak_spacing_cv"": [
    0.5172413789536267
  ],
  ""sample_entropy"": [
    0.3862951920004442
