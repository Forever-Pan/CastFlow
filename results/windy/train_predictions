  ""_llm_hints"": {{
    ""level_baseline"": ""Use median for bias correction if model prediction deviates significantly"",
    ""volatility"": ""Expand confidence interval if mad is high"",
    ""periodicity"": ""Inject periodic pattern if acf_s is high but model output is flat"",
    ""predictability"": ""Use conservative strategy if spec_entropy is high (noisy signal)""
  }}
