    ""direct_radiation"": 0.041437,
    ""wind_direction_80m"": -0.439478,
    ""wind_speed_80m"": -0.095898,
    ""temperature_2m"": 0.381926,
    ""relative_humidity_2m"": -0.409477,
    ""precipitation"": -0.196772
